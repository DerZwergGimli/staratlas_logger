{
  "compilerOptions": {
    /* Skip type checking all .d.ts files. */
    "rootDir": "app",
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
